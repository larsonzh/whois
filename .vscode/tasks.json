{
	"version": "2.0.0",
	"inputs": [
		{
			"id": "commitMsg",
			"type": "promptString",
			"description": "Commit message",
			"default": "chore: quick push"
		},
		{
			"id": "tagName",
			"type": "promptString",
			"description": "Tag name (e.g., v3.1.10)",
			"default": "v3.1.10"
		},
		{
			"id": "releaseVersion",
			"type": "promptString",
			"description": "Release version (X.Y.Z, no leading v)",
			"default": "3.2.3"
		},
		{
			"id": "tagMsg",
			"type": "promptString",
			"description": "Tag message",
			"default": "Release"
		},
		{
			"id": "releaseName",
			"type": "promptString",
			"description": "Release display name (GitHub/Gitee)",
			"default": "whois v3.2.3"
		},
		{
			"id": "skipTag",
			"type": "pickString",
			"description": "Skip creating/pushing git tag?",
			"options": ["false", "true"],
			"default": "false"
		},
		{
			"id": "rbHost",
			"type": "promptString",
			"description": "Remote build host (SSH), e.g. your.remote.host",
			"default": "10.0.0.199"
		},
		{
			"id": "rbUser",
			"type": "promptString",
			"description": "Remote SSH user",
			"default": "larson"
		},
		{
			"id": "rbKey",
			"type": "promptString",
			"description": "Private key path (on this PC; Git Bash style, e.g. /c/Users/you/.ssh/id_rsa)",
			"default": "/c/Users/妙妙呜/.ssh/id_rsa"
		},
		{
			"id": "rbSmoke",
			"type": "pickString",
			"description": "Run smoke tests on remote artifacts?",
			"options": [
				"1",
				"0"
			],
			"default": "1"
		},
		{
			"id": "rbQueries",
			"type": "promptString",
			"description": "Smoke queries (space-separated)",
			"default": "8.8.8.8 1.1.1.1 10.0.0.8"
		},
		{
			"id": "rbSmokeArgs",
			"type": "promptString",
			"description": "Additional Args for Smoke (-a). 留空视为无额外参数；示例：-g Domain|Registrar|Name Server|DNSSEC",
			"default": ""
		},
		{
			"id": "rbGolden",
			"type": "pickString",
			"description": "Enable golden check on fetched smoke log? (-G)",
			"options": ["1", "0"],
			"default": "1"
		},
		{
			"id": "rbCflagsExtra",
			"type": "promptString",
			"description": "Per-arch CFLAGS_EXTRA override for make (e.g., -O3 -s). 留空视为不传",
			"default": ""
		},
		{
			"id": "rbOptProfile",
			"type": "pickString",
			"description": "Optional OPT_PROFILE for make (lto-auto/lto-serial/small/NONE)",
			"options": ["lto-auto", "lto-serial", "small", "NONE"],
			"default": "lto-auto"
		},
		{
			"id": "rbSyncDir",
			"type": "promptString",
			"description": "Local sync dir(s) for statics (Git Bash path). 支持多个路径，用 ';' 或 ',' 分隔。",
			"default": "/d/LZProjects/lzispro/release/lzispro/whois;/d/LZProjects/whois/release/lzispro/whois"
		},
		{
			"id": "rbBatchLtoProfile",
			"type": "pickString",
			"description": "Opt profile for Batch Strategy Golden (lto) task",
			"options": ["lto-auto", "lto-serial"],
			"default": "lto-auto"
		},
		{
			"id": "rbSelftestLtoProfile",
			"type": "pickString",
			"description": "Opt profile for Selftest Golden Suite (prefilled, lto) task",
			"options": ["lto-auto", "lto-serial"],
			"default": "lto-auto"
		},
		{
			"id": "buildSync",
			"type": "pickString",
			"description": "Run remote build + smoke + sync & push statics?",
			"options": ["true", "false"],
			"default": "true"
		},
		{
			"id": "goldenRawLog",
			"type": "promptString",
			"description": "Raw-mode smoke log path (LATEST/AUTO to auto-pick; blank to skip)",
			"default": "./out/artifacts/batch_raw/<timestamp>/build_out/smoke_test.log"
		},
		{
			"id": "goldenHealthLog",
			"type": "promptString",
			"description": "Health-first smoke log path (LATEST/AUTO to auto-pick; blank to skip)",
			"default": "./out/artifacts/batch_health/<timestamp>/build_out/smoke_test.log"
		},
		{
			"id": "goldenPlanLog",
			"type": "promptString",
			"description": "Plan-A smoke log path (LATEST/AUTO to auto-pick; blank to skip)",
			"default": "./out/artifacts/batch_plan/<timestamp>/build_out/smoke_test.log"
		},
		{
			"id": "goldenPlanBLog",
			"type": "promptString",
			"description": "Plan-B smoke log path (LATEST/AUTO to auto-pick; blank to skip)",
			"default": "./out/artifacts/batch_planb/<timestamp>/build_out/smoke_test.log"
		},
		{
			"id": "goldenExtraArgs",
			"type": "promptString",
			"description": "Extra golden_check args (e.g., --strict). 输入 NONE 视为无额外参数。",
			"default": "NONE"
		},
		{
			"id": "prefLabels",
			"type": "promptString",
			"description": "Preference labels for golden pref checks (comma list, e.g., v4-then-v6-hop0; 输入 NONE 视为跳过)",
			"default": "NONE"
		},
		{
			"id": "selftestActions",
			"type": "promptString",
			"description": "Selftest actions for golden (comma list, e.g., force-suspicious,8.8.8.8)",
			"default": "force-suspicious,8.8.8.8;force-private,10.0.0.8;injection-view-fallback"
		},
		{
			"id": "selftestSmokeArgs",
			"type": "promptString",
			"description": "Smoke args for selftest run (-a). 留空使用默认；示例：--debug --retry-metrics --dns-cache-stats --selftest-force-suspicious 8.8.8.8 --selftest-force-private 10.0.0.8",
			"default": "--debug --retry-metrics --dns-cache-stats --selftest-force-suspicious 8.8.8.8 --selftest-force-private 10.0.0.8 --selftest-registry"
		},
		{
			"id": "selftestSmokeExtra",
			"type": "promptString",
			"description": "Extra smoke args for remote selftest run (leave empty to skip)",
			"default": ""
		},
		{
			"id": "selftestExpectations",
			"type": "promptString",
			"description": "Semicolon list for --expect spec (e.g., action=force-suspicious,query=8.8.8.8)",
			"default": "action=force-suspicious,query=8.8.8.8;action=force-private,query=10.0.0.8;action=injection-view-fallback;action=batch-registry-default;action=batch-registry-set-active;action=batch-registry-override-pick;action=batch-registry-override-on-result"
		},
		{
			"id": "selftestErrors",
			"type": "promptString",
			"description": "Semicolon list of regexes required via --require-error (NONE to skip)",
			"default": "Suspicious query detected;private IP address"
		},
		{
			"id": "selftestTags",
			"type": "promptString",
			"description": "Semicolon list of component:regex pairs for --require-tag (NONE to skip)",
			"default": "SELFTEST:action=force-(suspicious|private)"
		},
		{
			"id": "selftestPlanBTags",
			"type": "promptString",
			"description": "Plan-B only component:regex pairs for --require-tag (NONE to skip)",
			"default": "DNS-BATCH:action=plan-b-hit;DNS-BATCH:action=plan-b-stale;DNS-BATCH:action=plan-b-empty;DNS-BATCH:action=plan-b-fallback;DNS-BATCH:action=plan-b-force-start"
		},
		{
			"id": "selftestNoGoldenToggle",
			"type": "pickString",
			"description": "selftest_golden_suite.ps1 -NoGoldenToggle (true/false)",
			"options": ["false", "true"],
			"default": "false"
		},
		{
			"id": "stressBinary",
			"type": "promptString",
			"description": "Path to whois binary for stress (posix path preferred)",
			"default": "release/lzispro/whois/whois-win64.exe"
		},
		{
			"id": "stressBatchInput",
			"type": "promptString",
			"description": "Batch input file for stress (posix path)",
			"default": "testdata/queries.txt"
		},
		{
			"id": "stressOutDir",
			"type": "promptString",
			"description": "Output directory for stress logs",
			"default": "./out/stress"
		},
		{
			"id": "stressExtraArgs",
			"type": "promptString",
			"description": "Extra args for stress run (-B). 输入 NONE 视为无额外参数",
			"default": "--debug --retry-metrics --dns-cache-stats --dns-family-mode interleave-v4-first"
		},
		{
			"id": "stressRequireTags",
			"type": "promptString",
			"description": "Comma list for --require-tags (leave NONE to skip)",
			"default": "[DNS-CACHE-SUM],[NET-PROBE],[RETRY-METRICS]"
		},
		{
			"id": "stressGoldenExtraArgs",
			"type": "promptString",
			"description": "Extra args passed to golden_check.sh (NONE to skip)",
			"default": "--skip-header-tail --skip-redirect-line"
		},
		{
			"id": "redirectBinary",
			"type": "promptString",
			"description": "Binary path for redirect matrix test",
			"default": "${workspaceFolder}\\release\\lzispro\\whois\\whois-win64.exe"
		},
		{
			"id": "redirectOutDir",
			"type": "promptString",
			"description": "Output directory for redirect matrix report",
			"default": "${workspaceFolder}\\out\\artifacts\\redirect_matrix"
		},
		{
			"id": "redirectRirIpPref",
			"type": "promptString",
			"description": "--rir-ip-pref value (NONE to skip)",
			"default": "arin=ipv6"
		},
		{
			"id": "redirectPreferIpv4",
			"type": "pickString",
			"description": "Enable --prefer-ipv4?",
			"options": ["true", "false"],
			"default": "true"
		},
		{
			"id": "redirectSaveLogs",
			"type": "pickString",
			"description": "Save per-case logs?",
			"options": ["true", "false"],
			"default": "true"
		},
		{
			"id": "redirect10x6Binary",
			"type": "promptString",
			"description": "Binary path for 10x6 redirect matrix",
			"default": "${workspaceFolder}\\release\\lzispro\\whois\\whois-win64.exe"
		},
		{
			"id": "redirect10x6OutDirRoot",
			"type": "promptString",
			"description": "Output root for 10x6 logs",
			"default": "${workspaceFolder}\\out\\artifacts\\redirect_matrix_10x6"
		},
		{
			"id": "redirect10x6RirIpPref",
			"type": "promptString",
			"description": "--rir-ip-pref value (NONE to skip) for 10x6",
			"default": "arin=ipv6"
		},
		{
			"id": "redirect10x6PreferIpv4",
			"type": "pickString",
			"description": "Enable --prefer-ipv4 for 10x6?",
			"options": ["true", "false"],
			"default": "true"
		},
		{
			"id": "redirect10x6ShowExtraBodies",
			"type": "pickString",
			"description": "Enable --show-non-auth-body and --show-post-marker-body for 10x6?",
			"options": ["false", "true"],
			"default": "false"
		},
		{
			"id": "redirect10x6HideFailureBody",
			"type": "pickString",
			"description": "Enable --hide-failure-body for 10x6?",
			"options": ["false", "true"],
			"default": "false"
		},
		{
			"id": "cidrBodyBinary",
			"type": "promptString",
			"description": "Binary path for CIDR body contract smoke",
			"default": "${workspaceFolder}\\release\\lzispro\\whois\\whois-win64.exe"
		},
		{
			"id": "cidrBodyOutDir",
			"type": "promptString",
			"description": "Output directory for CIDR body contract smoke",
			"default": "${workspaceFolder}\\out\\artifacts\\cidr_body_contract"
		},
		{
			"id": "cidrBundleCasesFile",
			"type": "promptString",
			"description": "Cases file for CIDR contract bundle matrix step (TSV/CSV)",
			"default": "testdata/cidr_matrix_cases_draft.tsv"
		},
		{
			"id": "cidrBundleSummaryOutDir",
			"type": "promptString",
			"description": "Summary output directory for CIDR contract bundle",
			"default": "${workspaceFolder}\\out\\artifacts\\cidr_bundle"
		},
		{
			"id": "rirFailureInputRoots",
			"type": "promptString",
			"description": "Input roots for RIR failure raw extractor (semicolon-separated)",
			"default": "tmp/logs"
		},
		{
			"id": "rirFailureMaxExamples",
			"type": "promptString",
			"description": "Max examples per RIR in extractor report",
			"default": "12"
		}
	],
	"tasks": [
		{
			"label": "Git: Quick Push",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\dev\\quick_push.ps1",
				"-Message",
				"${input:commitMsg}"
			],
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "Remote: Build and Sync whois statics",
			"type": "process",
			"command": "C:\\Program Files\\Git\\bin\\bash.exe",
			"args": [
				"-lc",
				"tools/remote/remote_build_and_test.sh -H ${input:rbHost} -u ${input:rbUser} -k '${input:rbKey}' -r ${input:rbSmoke} -q '${input:rbQueries}' -s '${input:rbSyncDir}' -P 1 -a '${input:rbSmokeArgs}' -G ${input:rbGolden} -E '${input:rbCflagsExtra}' -O '${input:rbOptProfile}'"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "Remote: Build (Fast x86_64+win64)",
			"type": "process",
			"command": "C:\\Program Files\\Git\\bin\\bash.exe",
			"args": [
				"-lc",
				"tools/remote/remote_build_and_test.sh -H ${input:rbHost} -u ${input:rbUser} -k '${input:rbKey}' -t 'x86_64 win64' -w 0 -r ${input:rbSmoke} -q '${input:rbQueries}' -a '${input:rbSmokeArgs}' -G ${input:rbGolden} -E '${input:rbCflagsExtra}' -O '${input:rbOptProfile}' -L 0"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "Test: Redirect Matrix (IPv4)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\redirect_matrix_test.ps1"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test"
		},
		{
			"label": "Test: Redirect Matrix (IPv4, Params)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\redirect_matrix_test.ps1",
				"-BinaryPath",
				"${input:redirectBinary}",
				"-OutDir",
				"${input:redirectOutDir}",
				"-RirIpPref",
				"${input:redirectRirIpPref}",
				"-PreferIpv4",
				"${input:redirectPreferIpv4}",
				"-SaveLogs",
				"${input:redirectSaveLogs}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test"
		},
		{
			"label": "Test: Redirect Matrix (CIDR Draft TSV)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\redirect_matrix_test.ps1",
				"-BinaryPath",
				"${input:redirectBinary}",
				"-OutDir",
				"${input:redirectOutDir}",
				"-RirIpPref",
				"${input:redirectRirIpPref}",
				"-PreferIpv4",
				"${input:redirectPreferIpv4}",
				"-SaveLogs",
				"${input:redirectSaveLogs}",
				"-CasesFile",
				"testdata/cidr_matrix_cases_draft.tsv"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test",
			"problemMatcher": []
		},
		{
			"label": "Test: Redirect Matrix (CIDR Draft TSV, prefilled)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\redirect_matrix_test.ps1",
				"-BinaryPath",
				"${workspaceFolder}\\release\\lzispro\\whois\\whois-win64.exe",
				"-OutDir",
				"${workspaceFolder}\\out\\artifacts\\redirect_matrix",
				"-RirIpPref",
				"arin=ipv6",
				"-PreferIpv4",
				"true",
				"-SaveLogs",
				"true",
				"-CasesFile",
				"testdata/cidr_matrix_cases_draft.tsv"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test",
			"problemMatcher": []
		},
		{
			"label": "Test: Redirect Matrix (10x6)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\redirect_matrix_10x6.ps1",
				"-BinaryPath",
				"${input:redirect10x6Binary}",
				"-OutDirRoot",
				"${input:redirect10x6OutDirRoot}",
				"-RirIpPref",
				"${input:redirect10x6RirIpPref}",
				"-PreferIpv4",
				"${input:redirect10x6PreferIpv4}",
				"-ShowExtraBodies",
				"${input:redirect10x6ShowExtraBodies}",
				"-HideFailureBody",
				"${input:redirect10x6HideFailureBody}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test"
		},
		{
			"label": "Test: CIDR Body Contract",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\cidr_body_contract_smoke.ps1",
				"-BinaryPath",
				"${input:cidrBodyBinary}",
				"-OutDir",
				"${input:cidrBodyOutDir}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test",
			"problemMatcher": []
		},
		{
			"label": "Test: CIDR Body Contract (prefilled)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\cidr_body_contract_smoke.ps1",
				"-BinaryPath",
				"${workspaceFolder}\\release\\lzispro\\whois\\whois-win64.exe",
				"-OutDir",
				"${workspaceFolder}\\out\\artifacts\\cidr_body_contract"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test",
			"problemMatcher": []
		},
		{
			"label": "Test: CIDR Contract Bundle",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\run_cidr_contract_bundle.ps1",
				"-BinaryPath",
				"${input:cidrBodyBinary}",
				"-BodyOutDir",
				"${input:cidrBodyOutDir}",
				"-MatrixOutDir",
				"${input:redirectOutDir}",
				"-SummaryOutDir",
				"${input:cidrBundleSummaryOutDir}",
				"-RirIpPref",
				"${input:redirectRirIpPref}",
				"-PreferIpv4",
				"${input:redirectPreferIpv4}",
				"-SaveLogs",
				"${input:redirectSaveLogs}",
				"-CasesFile",
				"${input:cidrBundleCasesFile}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test",
			"problemMatcher": []
		},
		{
			"label": "Test: CIDR Contract Bundle (prefilled)",
			"type": "shell",
			"command": "powershell",
			"args": [
				"-NoProfile",
				"-ExecutionPolicy",
				"Bypass",
				"-File",
				"${workspaceFolder}\\tools\\test\\run_cidr_contract_bundle.ps1",
				"-BinaryPath",
				"${workspaceFolder}\\release\\lzispro\\whois\\whois-win64.exe",
				"-BodyOutDir",
				"${workspaceFolder}\\out\\artifacts\\cidr_body_contract",
				"-MatrixOutDir",
				"${workspaceFolder}\\out\\artifacts\\redirect_matrix",
				"-SummaryOutDir",
				"${workspaceFolder}\\out\\artifacts\\cidr_bundle",
				"-RirIpPref",
				"arin=ipv6",
				"-PreferIpv4",
				"true",
				"-SaveLogs",
				"true",
				"-CasesFile",
				"testdata/cidr_matrix_cases_draft.tsv"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "test",
			"problemMatcher": []
		},
		{
			"label": "Remote: Build (Strict Version)",
			"type": "process",
			"command": "C:\\Program Files\\Git\\bin\\bash.exe",
			"args": [
				"-lc",
				"tools/remote/remote_build_and_test.sh -H ${input:rbHost} -u ${input:rbUser} -k '${input:rbKey}' -r ${input:rbSmoke} -q '${input:rbQueries}' -s '${input:rbSyncDir}' -P 1 -a '${input:rbSmokeArgs}' -G ${input:rbGolden} -E '${input:rbCflagsExtra}' -O '${input:rbOptProfile}'"
			],
			"options": {
